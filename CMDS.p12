#!/bin/bash
SDIR="$( cd "$( dirname "$0" )" && pwd )"

if [ ! -e PROJNAME ]; then
    python3 $SDIR/extractProjectIDFromPath.py $PWD | sed 's/Proj_/p/' >PROJNAME
fi

export SDIR

time Rscript --no-save $SDIR/doSeuratV5_01.R $* ../cellRanger/s_*
